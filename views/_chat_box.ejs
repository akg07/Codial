<link rel="stylesheet" href="/css/chat_box.css">

<% if(locals.user) { %>
    <div id="user-chat-box">
        <ul id="chat-message-list-container">
            <li class="recieved-message">
                <span>Recieved Message</span>
            </li>
            <li class="sent-message">
                <span>Sent Message</span>
            </li>
        </ul>

        <div id="chat-input-container">
            <input type="text" id="chat-message-input" placeholder="Type message here...">
            <button id="send-message">Send</button>
        </div>
    </div>
<% } %>